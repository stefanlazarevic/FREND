mixin gmap__script
  script.
    function initMap() {
      var uluru = {lat: -25.363, lng: 131.044};
      var map = new google.maps.Map(document.querySelector('[data-map="google"]'), {
        zoom: 4,
        scrollwheel: false,
        center: uluru
      });
      var marker = new google.maps.Marker({
        position: uluru,
        map: map
      });
      console.log('Google Map initialized.');
    }
